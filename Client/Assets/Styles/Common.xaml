    <!--
        Репозиторий стилей элементов.
    
        Использование в XAML:
        (следует использовать относительные пути)
    
        <UserControl.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="/l-pack_erp;component/Assets/Styles/Common.xaml" />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </UserControl.Resources>
            
    
        Contents        
    
        100 General
        200 Grids
            210 Containers 
            220 Controls
        300 Forms
            310 Containers
            320 Controls
        400 Special Controls 
            410 Chat  
            420 Shipments Plan, services
            430 Shipments Monitor
            460 RollReel
            470 Complectation
