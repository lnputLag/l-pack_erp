                                HorizontalAlignment="Left" 
                                IsReadOnly="{TemplateBinding IsReadOnly}"
                                Template="{StaticResource DataGridColumnSelectBoxText}" 
                                VerticalAlignment="Top" 
                                Visibility="Hidden" 
                                Background="Transparent"                                
                            />

