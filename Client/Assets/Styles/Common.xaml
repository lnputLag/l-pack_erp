    <Style x:Key="DataGridTouch" TargetType="{x:Type DataGrid}" BasedOn="{StaticResource DataGridMain}" >
        <Setter Property="RowHeight" Value="30"/>
        <Setter Property="FontSize" Value="18"/>
        <Setter Property="FontWeight" Value="SemiBold"/>
