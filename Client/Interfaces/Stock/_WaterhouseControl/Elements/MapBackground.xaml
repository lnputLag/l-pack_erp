             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client.Interfaces.Stock"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button BorderThickness="5" BorderBrush="Black" Grid.Column="1" Grid.Row="1"  Name="Button" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="1" Padding="0">

